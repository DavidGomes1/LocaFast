{
  "version": 2,
  "builds": [
    {
      "src": "frontWeb/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "locaFastApi/src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "locaFastApi/src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "frontWeb/dist/$1"
    }
  ]
}
