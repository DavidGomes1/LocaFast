{
  "name": "amandapuceixo4",
  "builds": [
    {
      "src": "frontWeb/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontWeb/dist"
      }
    },
    {
      "src": "locaFastApi/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/locaFastApi/src/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontWeb/dist/$1"
    }
  ]
}
